# Настройки базы данных MySQL
DB_HOST=your_mysql_host
DB_PORT=3306
DB_USER=your_mysql_user
DB_PASSWORD=your_mysql_password
DB_NAME=Voice_DB

# SSH сервер с аудиофайлами
SSH_HOST=your_ssh_host
SSH_USER=your_ssh_user
SSH_PASSWORD=your_ssh_password
SSH_REMOTE_PATH=/var/spool/asterisk/monitor/2024/10

# Сервер распознавания речи Whisper
WHISPER_API_URL=http://your_whisper_server:7860/

# HuggingFace токен для диаризации (получить на https://huggingface.co/settings/tokens)
HF_TOKEN=your_huggingface_token

# Локальная временная директория для скачанных файлов
LOCAL_TEMP_DIR=TEMP 